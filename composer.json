{
    "name": "fm-labs/cakephp3-rbac",
    "description": "Role based access control plugin for CakePHP3",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Rbac\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rbac\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
